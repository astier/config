{
  "shebang": {
    "prefix": ["s"],
    "body": ["#!/usr/bin/env sh\n\n"]
  },
  "alias": {
    "prefix": ["a"],
    "body": ["alias $1=\"$0\""]
  },
  "function-oneliner": {
    "prefix": ["ff"],
    "body": ["$1() { $0; }"]
  },
  "function": {
    "prefix": ["f"],
    "body": ["$1() {\n\t$0\n}"]
  }
}
