{
  "autocmd": {
    "prefix": ["a"],
    "body": ["autocmd "]
  },
  "autocmd group": {
    "prefix": ["ag"],
    "body": ["autocmd group "]
  },
  "nnoremap": {
    "prefix": ["n"],
    "body": ["nnoremap "]
  },
  "function": {
    "prefix": ["f"],
    "body": ["fun! $1()\n\t$0\nendfun"]
  },
  "if": {
    "prefix": ["i"],
    "body": ["if $0\n\t\nen"]
  },
  "packadd": {
    "prefix": ["pa"],
    "body": ["packadd! "]
  },
  "plug": {
    "prefix": ["p"],
    "body": ["Plug '$0'"]
  },
  "highlight": {
    "prefix": ["h"],
    "body": ["highlight $1 ctermbg=none ctermfg=none cterm=none \" $0"]
  },
  "highlight link": {
    "prefix": ["hl"],
    "body": ["highlight! link $1 \" $0"]
  },
  "highight link none": {
    "prefix": ["hln"],
    "body": ["highlight! link $1 none \" $0"]
  }
}
