{
    "cSpell.userWords": [
        "datapoint",
        "datapoints",
        "dataset",
        "datasets",
        "keras",
        "lstm",
        "numpy",
        "rint"
    ],

    "breadcrumbs.enabled": false,
    "breadcrumbs.filePath": "last",
    "breadcrumbs.icons": false,
    "debug.toolBarLocation": "docked",
    "diffEditor.renderSideBySide": false,
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "monospace",
    "editor.fontSize": 13,
    "editor.formatOnPaste": true,
    "editor.glyphMargin": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.hover.enabled": false,
    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderLineHighlight": "none",
    "editor.rulers": [ 88 ],
    "editor.suggest.localityBonus": true,
    "editor.suggest.maxVisibleSuggestions": 8,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 88,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "files.autoSave": "afterDelay",
    "files.enableTrash": false,
    "files.eol": "\n",
    "files.exclude": { "**/__pycache__": true },
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.simpleDialog.enable": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.rebaseWhenSync": true,
    "keyboard.dispatch": "keyCode",
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.latex.outDir": "%DIR%/tex",
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.pdf.viewer": "browser",
    "python.autoComplete.addBrackets": true,
    "python.jediEnabled": false,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.inheritEnv": true,
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.leader": " ",
    "vim.visualstar": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "toggle",
    "window.openWithoutArgumentsInNewWindow": "off",
    "window.title": "VSCode",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Nord",
    "workbench.commandPalette.history": 8,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.openSideBySideDirection": "down",
    "workbench.enableExperiments": false,
    "workbench.iconTheme": null,
    "workbench.list.horizontalScrolling": true,
    "workbench.octiconsUpdate.enabled": true,
    "workbench.quickOpen.closeOnFocusLost": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": false,

    "vim.normalModeKeyBindingsNonRecursive": [

        { "before": [ ":" ], "commands": [ "workbench.action.showCommands" ] },
        { "before": [ "<leader>", "h", "u" ], "commands": [ "git.revertSelectedRanges" ] },
        { "before": [ "cr" ], "after": [ "o", "<esc>" ] },

        { "before": [ "$" ], "after": [ "g", "$" ] },
        { "before": [ "0" ], "after": [ "g", "0" ] },
        { "before": [ "j" ], "after": [ "g", "j" ] },
        { "before": [ "k" ], "after": [ "g", "k" ] }

    ]
}
