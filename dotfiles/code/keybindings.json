[

    { "key": "alt+e", "command": "workbench.action.previousEditor" },
    { "key": "alt+r", "command": "workbench.action.nextEditor" },
    { "key": "alt+tab", "command": "workbench.action.closeActiveEditor" },

    { "key": "alt+d", "command": "scrollLineUp", "when": "editorTextFocus" },
    { "key": "alt+f", "command": "scrollLineDown", "when": "editorTextFocus" },

    { "key": "shift+alt+f", "command": "workbench.action.quickOpen", "when": "editorTextFocus" },

