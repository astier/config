articlelist-format "%f %t"
articlelist-title-format "%T"
auto-reload yes
color info blue default bold
color listfocus yellow default bold
color listfocus_unread red default bold
feedlist-format "%n  %t"
feedlist-title-format "FEEDS"
help-title-format "HELP"
itemview-title-format "%T"
max-items 8
show-keymap-hint no

bind-key a toggle-article-read
bind-key j down
bind-key k up
bind-key j next articlelist
bind-key k prev articlelist
bind-key l open
bind-key h quit
bind-key g home
bind-key G end
bind-key n next-unread
bind-key N prev-unread
