xkb_keymap {
    xkb_keycodes { include "evdev+aliases(qwerty)" };
    xkb_types    { include "complete" };
    xkb_compat   { include "complete" };
    xkb_symbols  {
        include "pc+us(altgr-intl)+inet(evdev)+capslock(swapescape)"
        key <AC10> { [ Return ] };
        key <RTRN> { [ semicolon ] };
    };
};
